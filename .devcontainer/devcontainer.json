{
    "$schema": "https://raw.githubusercontent.com/devcontainers/spec/main/schemas/devContainer.schema.json",
    "name": "filehistogram.ghc",
    "image": "localhost/ghc",
    "runArgs": [
        "--userns=keep-id:uid=${localEnv:$CONTAINER_USERID},gid=${localEnv:$CONTAINER_USERID}"
    ],
    "mounts": [
        "source=${localEnv:$LINUX_CONFIG}/build-with-docker/var/lib/apt,target=/var/lib/apt,type=bind,z,consistency=cached",
        "source=${localEnv:$LINUX_CONFIG}/build-with-docker/var/cache/apt,target=/var/cache/apt,type=bind,z,consistency=cached",
        "source=${localEnv:$LINUX_CONFIG}/build-with-docker/var/lib/dnf,target=/var/lib/dnf,type=bind,z,consistency=cached",
        "source=${localEnv:$LINUX_CONFIG}/build-with-docker/var/cache/dnf,target=/var/cache/dnf,type=bind,z,consistency=cached",
        "source=${localEnv:$LINUX_CONFIG}/build-with-docker/.cargo,target=/home/vscode/.cargo,type=bind,z,consistency=cached",
        "source=${localEnv:$LINUX_CONFIG}/build-with-docker/.rustup,target=/home/vscode/.rustup,type=bind,z,consistency=cached",
        "source=${localEnv:$LINUX_CONFIG}/build-with-docker/.ghcup,target=/home/vscode/.ghcup,type=bind,z,consistency=cached",
        "source=${localEnv:$LINUX_CONFIG}/build-with-docker/.stack,target=/home/vscode/.stack,type=bind,z,consistency=cached",
        "source=${localEnv:$LINUX_CONFIG}/build-with-docker/.cabal,target=/home/vscode/.cabal,type=bind,z,consistency=cached",
        "source=${localEnv:$LINUX_CONFIG}/dev-in-container/config.yaml,target=/home/vscode/.stack/config.yaml,type=bind,z,consistency=cached",
        "source=${localEnv:$LINUX_CONFIG}/dev-in-container/cabal/config,target=/home/vscode/.cabal/config,type=bind,z,consistency=cached",
        "source=${localEnv:$LINUX_CONFIG}/dev-in-container/ghcup/env,target=/home/vscode/.ghcup/env,type=bind,z,consistency=cached"
    ],
    "workspaceFolder": "${localEnv:$FILEHISTOGRAM}",
    "workspaceMount": "source=${localEnv:$FILEHISTOGRAM},target=/workspaces/ghc,type=bind,consistency=cached",
    "remoteUser": "vscode",
    "customizations": {
        "vscode": {
            "extensions": [

            ]
        }
    }
}
